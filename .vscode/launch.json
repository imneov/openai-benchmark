    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Module",
            "type": "debugpy",
            "request": "launch",
            "module": "benchmark.bench",
            "args": [
                "load",
                "--deployment",
                "mistralai/Mistral-7B-v0.1",
                "--rate",
                "60",
                "--retry",
                "exponential",
                "http://127.0.0.1:8080"
            ],
            "justMyCode": false,
            "env": {
                "OPENAI_API_KEY":"aaa"
            }
        }
    ]
}